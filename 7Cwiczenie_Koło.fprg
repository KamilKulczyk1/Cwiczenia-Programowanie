    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;Podaj koordynaty A i B twojego punktu oraz promie&#324; ko&#322;a&quot;" newline="True"/>
            <declare name="a, b, x, y, r" type="Integer" array="False" size=""/>
