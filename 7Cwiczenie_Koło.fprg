                    <output expression="&quot;Punkt le&#380;y na lini lub poza lini&#261; ko&#322;a&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Punkt le&#380;y w obr&#281;bie ko&#322;a&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
